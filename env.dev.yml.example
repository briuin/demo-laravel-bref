# Development environment variables - EXAMPLE
# Copy this file to env.dev.yml and customize for your needs

APP_ENV: local
APP_KEY: # Generate with: php artisan key:generate --show
APP_DEBUG: true
APP_URL: https://dev-api.yourapp.com

# Database configuration for development
DB_CONNECTION: mysql
DB_HOST: your-dev-db-host.amazonaws.com
DB_DATABASE: laravel_dev
DB_USERNAME: laravel_user
DB_PASSWORD: your-dev-password

# Cache and Session
CACHE_DRIVER: dynamodb
SESSION_DRIVER: dynamodb

# Queue
QUEUE_CONNECTION: sqs

# Mail
MAIL_MAILER: ses

# Logging
LOG_CHANNEL: stderr
LOG_LEVEL: debug

# Additional development settings
DEBUGBAR_ENABLED: true
TELESCOPE_ENABLED: true
